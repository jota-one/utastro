{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@styles/*": ["./src/styles/*"],
    },
    "jsx": "preserve",
    "jsxImportSource": "astro",
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist", "pb"],
}
